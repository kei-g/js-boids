{
  "compilerOptions": {
    "alwaysStrict": true,
    "downlevelIteration": true,
    "emitDecoratorMetadata": false,
    "experimentalDecorators": false,
    "lib": [
      "DOM",
      "ESNext",
    ],
    "module": "ES2022",
    "moduleResolution": "node",
    "newLine": "lf",
    "pretty": true,
    "removeComments": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": false,
    "target": "ES2021",
  },
  "exclude": [],
  "include": [
    "src",
  ],
}
